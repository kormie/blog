<div class ="container">
  <p id="notice"><%= notice %></p>
  <div class="content">
    <div class="article">
      <h2>
      <%= @article.name %>
      </h2>

      <p>
      <%= @article.content %>
      </p>
    </div>
    <div class="comments">
      <%= raw render_disqus_javascript %>
    </div>
  </div>
  <%= link_to 'Edit', edit_article_path(@article) %> |
  <%= link_to 'Back', articles_path %>
</div>
